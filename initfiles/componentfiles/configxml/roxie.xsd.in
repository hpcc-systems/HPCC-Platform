      <xs:annotation>
        <xs:appinfo>
          <tooltip>Number of parallel threads to use for in-memory aggregate processing. Set to 0 to use one per CPU, 1 to disable parallel processing of in-memory aggregates</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="perChannelFlowLimit" type="xs:nonNegativeInteger" use="optional" default="10">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Number of pending queries permitted per channel (per active activity) before blocking</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pingInterval" type="xs:nonNegativeInteger" use="optional" default="60">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Interval (in seconds) between Roxie server ping tests</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="preabortIndexReadsThreshold" type="xs:nonNegativeInteger" use="optional" default="100">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Use seek to precheck keyed limits (i.e. assume ,COUNT) on index reads if limit greater than this value</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="preabortKeyedJoinsThreshold" type="xs:nonNegativeInteger" use="optional" default="100">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Use seek to precheck limits on keyed joins if limit greater than this value</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="simpleLocalKeyedJoins" type="xs:boolean" use="optional" default="true">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Enable single-threaded local keyed joins</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="socketCheckInterval" type="xs:nonNegativeInteger" use="optional" default="5000">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>Interval (in milliseconds) between checks that client socket is still open</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="systemMonitorInterval" type="xs:nonNegativeInteger" use="optional" default="60000">
      <xs:annotation>
        <xs:appinfo>
          <tooltip>How often to send an "alive" message</tooltip>
        </xs:appinfo>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
</xs:schema>

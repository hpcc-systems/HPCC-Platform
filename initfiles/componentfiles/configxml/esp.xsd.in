                                    <xs:appinfo>
                                        <viewType>list</viewType>
                                        <title>URL Authentication</title>
                                    </xs:appinfo>
                                </xs:annotation>
                                <xs:complexType>
                                <!--DOC-Autobuild-code-->
				 <xs:annotation>
				    <xs:appinfo>
				       <docid>ESP.t1</docid>
				    </xs:appinfo>
     			         </xs:annotation> 
                                    <xs:attribute name="description" type="xs:string" use="optional">
                                        <xs:annotation>
                                            <xs:appinfo>
                                                <width>200</width>

<sect1 id="LN">
  <!-- DNT-Start --><title>LN</title><!-- DNT-End -->

  <!-- DNT-Start --><para><emphasis role="bold">LN<indexterm>
      <primary>LN</primary>
    </indexterm><indexterm>
      <primary>LN function</primary>
    </indexterm>(</emphasis><emphasis>n</emphasis><emphasis
  role="bold">)</emphasis></para><!-- DNT-End -->

  <informaltable colsep="1" frame="all" rowsep="1">
    <tgroup cols="2">

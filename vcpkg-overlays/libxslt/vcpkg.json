{
  "name": "libxslt",
  "version": "1.1.34",
  "port-version": 4,
  "description": "Libxslt is a XSLT library implemented in C for XSLT 1.0 and most of EXSLT",
  "homepage": "https://github.com/GNOME/libxslt",
  "license": null,
  "supports": "!uwp",
  "dependencies": [
    "liblzma",
    "libxml2"
  ],
  "default-features": [
    "crypto"
  ],
  "features": {
    "python": {
      "description": "Builds with python support (--with-python), !windows"
    },
    "plugins": {
      "description": "Build with plugins (--with-plugins), !windows"
    },
    "crypto": {
      "description": "Build with crypto support (--with-crypto), !windows",
      "dependencies": [
        "libgcrypt"
      ]
    }
  }
}
{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "hpcc-platform",
    "version": "8.12.0",
    "dependencies": [
        {
            "name": "apr",
            "platform": "@VCPKG_APR@"
        },
        {
            "name": "apr-util",
            "platform": "@VCPKG_APR@"
        },
        {
            "name": "aws-sdk-cpp",
            "default-features": false,
            "features": [
                "s3",
                "sqs"
            ],
            "platform": "@VCPKG_SQS@"
        },
        {
            "name": "azure-storage-blobs-cpp",
            "platform": "@VCPKG_AZURE@"
        },
        {
            "name": "azure-storage-files-shares-cpp",
            "platform": "@VCPKG_AZURE@"
        },
        {
            "name":"boost-regex",
            "platform": "@VCPKG_BOOST_REGEX@"
        },
        "boost-property-tree",
        {
            "name": "cpp-driver",
            "platform": "@VCPKG_CASSANDRAEMBED@"
        },
        {
            "name": "cpr",
            "platform": "@VCPKG_ELASTICSTACK_CLIENT@"
        },
        "curl",
        {
            "name": "elasticlient",
            "platform": "@VCPKG_ELASTICSTACK_CLIENT@ & !windows"
        },
        {
            "name": "h3",
            "platform": "@VCPKG_H3@"
        },
        {
            "name": "hiredis",
            "platform": "@VCPKG_REDIS@"
        },
        "icu",
        {
            "name": "jsoncpp",
            "platform": "@VCPKG_ELASTICSTACK_CLIENT@"
        },
        "jwt-cpp",
        {
            "name": "libarchive",
            "default-features": false,
            "features": [
                "bzip2"
            ]
        },
        {
            "name": "libcouchbase-cxx",
            "platform": "@VCPKG_COUCHBASEEMBED@"
        },
        {
            "name": "libgit2",
            "platform": "@VCPKG_GIT@"
        },
        {
            "name": "libiconv",
            "platform": "osx"
        },
        {
            "name": "libmemcached",
            "platform": "@VCPKG_MEMCACHED@ & !windows & !osx"
        },
        {
            "name": "libmysql",
            "platform": "@VCPKG_MYSQLEMBED@ & !(windows & x86)"
        },
        {
            "name": "librdkafka",
            "platform": "@VCPKG_KAFKA@"
        },
        {
            "name": "libuv",
            "platform": "@VCPKG_CASSANDRAEMBED@"
        },
        {
            "name": "libxml2",
            "platform": "@VCPKG_LIBXALAN@"
        },
        {
            "name": "libxslt",
            "default-features": false,
            "features": [],
            "platform": "@VCPKG_LIBXSLT@"
        },
        "libyaml",
        "lz4",
        {
            "name": "mongo-cxx-driver",
            "platform": "@VCPKG_MONGODBEMBED@"
        },
        "nlohmann-json",
        {
            "name": "nlp-engine",
            "platform": "@VCPKG_NLP@"
        },
        {
            "name": "numactl",
            "platform": "@VCPKG_NUMA@ & !windows & !osx"
        },
        {
            "name": "openblas",
            "features": [
                "dynamic-arch",
                "threads"
            ],
            "platform": "@VCPKG_ECLBLAS@ & !windows"
        },
        {
            "name": "openblas",
            "features": [
                "threads"
            ],
            "platform": "@VCPKG_ECLBLAS@ & windows"
        },
        {
            "name": "openldap",
            "platform": "@VCPKG_OPENLDAP@ & !windows"
        },
        {
            "name": "openssl",
            "platform": "@VCPKG_OPENSSL@"
        },
        {
            "name": "python3",
            "platform": "@VCPKG_PYTHON3@ & windows"
        },
        "rapidjson",
        {
            "name": "sqlite3",
            "platform": "@VCPKG_SQLITE3EMBED@"
        },
        {
            "name": "tbb",
            "platform": "@VCPKG_TBB@"
        },
        {
            "name": "winflexbison",
            "platform": "windows"
        },
        {
            "name": "zlib",
            "platform": "@VCPKG_ZLIB@"
        }
    ]
}
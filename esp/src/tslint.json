{
    /*
   * Possible values:
   * - the name of a built-in config
   * - the name of an NPM module which has a "main" file that exports a config object
   * - a relative path to a JSON file
   */
    "extends": "tslint:recommended",
    "rules": {
        /*
     * Any rules specified here will override those from the base config we are extending.
     */
        "prefer-for-of": false,
        "no-console": [
            false
        ],
        "radix": false,
        "interface-over-type-literal": false,
        "no-empty-interface": false,
        "no-unused-expression": false,
        "space-before-function-paren": false,
        "arrow-parens": false,
        "forin": false,
        "member-access": false,
        "variable-name": [
            false
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "curly": false,
        "member-ordering": [
            true
        ],
        "max-classes-per-file": [
            false
        ],
        "no-empty": false,
        "max-line-length": [
            false
        ],
        "only-arrow-functions": [
            false
        ],
        "no-namespace": [
            false
        ],
        "interface-name": [
            false
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type"
        ],
        "prefer-const": true,
        "no-string-literal": false
    },
    "jsRules": {
        /*
     * Any rules specified here will override those from the base config we are extending.
     */
        "curly": true
    },
    "rulesDirectory": [
        /*
     * A list of relative or absolute paths to directories that contain custom rules.
     * See the Custom Rules documentation below for more details.
     */
    ]
}
{
  "name": "eclwatch",
  "version": "1.0.0",
  "description": "'ECL Watch' Web interface for HPCC Platform.",
  "scripts": {
    "install-playwright-deps": "npx playwright install --with-deps",
    "clean": "rimraf ./build ./lib ./types ./src/nlsHPCCType.ts",
    "lint": "eslint ./eclwatch ./src ./src-dojo ./src-react",
    "lint-fix": "eslint --fix eclwatch/**/*.js src/**/*.ts src-react/**/*.ts?",
    "copy-res-eclwatch-img": "copyfiles -u 2 \"./eclwatch/img/**/*.{png,jpg,gif,svg}\" ./build/eclwatch/img/",
    "copy-res-eclwatch-ecl": "copyfiles -u 2 \"./eclwatch/ecl/**/*.*\" ./build/eclwatch/ecl/",
    "copy-res-dgrid-shim": "copyfiles -u 4 \"./node_modules/@hpcc-js/dgrid-shim/dist/index.min.js\" ./build/dgrid-shim/",
    "copy-res-dojo-img": "copyfiles -u 3 \"./node_modules/dojo/resources/blank.gif\" ./build/eclwatch/img/",
    "copy-res-dojo": "copyfiles -u 3 \"./node_modules/dojo/resources/**/*.{png,jpg,gif}\" ./build/node_modules/dojo/resources/",
    "copy-res-dojox": "copyfiles -u 5 \"./node_modules/dojox/widget/ColorPicker/images/**/*.{png,jpg,gif}\" ./build/eclwatch/img/",
    "copy-res-dgrid-css": "copyfiles -u 2 \"./dgrid/css/**/*\" ./lib/dgrid/css/",
    "copy-res-TopoJSON": "copyfiles -u 4 \"./node_modules/@hpcc-js/map/TopoJSON/**/*\" ./build/dist/TopoJSON/",
    "copy-res-font-awesome": "copyfiles -u 2 \"./node_modules/font-awesome/**/*\" ./build/dist/font-awesome/",
    "copy-res-fluentui-fonts": "copyfiles -f \"./node_modules/@fluentui/font-icons-mdl2/fonts/*\" ./build/dist/fluentui-fonts/",
    "copy-res-html": "copyfiles \"./*.{htm,html}\" ./build/",
    "copy-res": "run-p copy-res-eclwatch-img copy-res-eclwatch-ecl copy-res-dgrid-shim copy-res-dojo-img copy-res-dojo copy-res-dojox copy-res-dgrid-css copy-res-TopoJSON copy-res-font-awesome copy-res-fluentui-fonts copy-res-html",
    "compile": "tsc",
    "compile-watch": "npm run compile -- -w",
    "bundle": "node node_modules/webpack/bin/webpack.js --env production --config webpack.config.js",
    "bundle-dev": "node node_modules/webpack/bin/webpack.js --env development --config webpack.config.js",
    "bundle-watch": "npm run bundle-dev -- --watch",
    "build": "npm-run-all --parallel copy-res compile --serial bundle",
    "build-dev": "npm-run-all --parallel copy-res compile --serial bundle-dev",
    "dev-start-ws": "ws",
    "dev-start": "run-p bundle-watch dev-start-ws",
    "dev-start-verbose": "ws --verbose.include request response",
    "rm-hpcc": "rimraf ./node_modules/@hpcc-js",
    "start": "ws",
    "test": "npx playwright test",
    "test-dfuworkunits": "npx playwright test tests/v9-dfuworkunits.spec.ts",
    "test-ci": "cross-env CI=1 npx playwright test",
    "test-ci-local": "cross-env CIL=1 npx playwright test",
    "test-full": "cross-env FULL=1 npx playwright test",
    "test-codegen": "npx playwright codegen",
    "test-interactive": "npx playwright test --ui",
    "update": "npx npm-check-updates -u -t minor",
    "update-major": "npx npm-check-updates -u"
  },
  "main": "src/stub.js",
  "dependencies": {
    "@fluentui/react": "8.125.4",
    "@fluentui/react-components": "9.72.11",
    "@fluentui/react-nav-preview": "0.13.9",
    "@fluentui/react-datepicker-compat": "0.6.22",
    "@fluentui/react-experiments": "8.16.4",
    "@fluentui/react-hooks": "8.10.2",
    "@fluentui/react-icons-mdl2": "1.4.4",
    "@fluentui/react-migration-v8-v9": "9.10.1",
    "@fluentui/react-timepicker-compat": "0.4.28",
    "@hpcc-js/chart": "3.6.5",
    "@hpcc-js/codemirror": "3.6.9",
    "@hpcc-js/common": "3.6.5",
    "@hpcc-js/comms": "3.14.4",
    "@hpcc-js/dataflow": "3.0.1",
    "@hpcc-js/eclwatch": "3.5.10",
    "@hpcc-js/graph": "3.6.5",
    "@hpcc-js/html": "3.3.9",
    "@hpcc-js/layout": "3.5.4",
    "@hpcc-js/map": "3.4.10",
    "@hpcc-js/other": "3.4.10",
    "@hpcc-js/phosphor": "3.4.7",
    "@hpcc-js/timeline": "3.3.5",
    "@hpcc-js/util": "3.4.8",
    "@hpcc-js/wasm-duckdb": "1.17.0",
    "@hpcc-js/wasm-graphviz": "1.21.0",
    "clipboard": "2.0.11",
    "d3-dsv": "3.0.1",
    "dijit": "1.17.3",
    "dijit-themes": "1.17.3",
    "dojo": "1.17.3",
    "dojo-util": "1.17.3",
    "dojox": "1.17.3",
    "font-awesome": "4.7.0",
    "formik": "2.4.9",
    "octokit": "4.1.4",
    "put-selector": "0.3.6",
    "query-string": "9.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.71.1",
    "react-reflex": "4.2.7",
    "universal-router": "9.2.1",
    "xstyle": "0.3.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@playwright/test": "1.58.2",
    "@simbathesailor/use-what-changed": "2.0.0",
    "@types/dojo": "1.9.48",
    "@types/node": "22.19.11",
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/parser": "8.55.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "css-loader": "7.1.3",
    "dojo-webpack-plugin": "3.0.9",
    "eslint": "9.39.2",
    "eslint-plugin-eclwatch": "file:./eslint",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.5.0",
    "file-loader": "6.2.0",
    "local-web-server": "5.4.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.10",
    "source-map-loader": "5.0.0",
    "string-replace-loader": "3.3.0",
    "style-loader": "4.0.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "url-loader": "4.1.1",
    "webpack": "5.105.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3"
  },
  "author": "HPCC Systems",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hpcc-systems/HPCC-Platform"
  }
}